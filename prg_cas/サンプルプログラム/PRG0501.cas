PRG0501  START
         RPUSH
;
         LAD     GR1,8           ; GR1初期化
LOOP     LD      GR2,='0'        ; 繰り返し処理の始まり
         ADDA    GR2,GR1         ; 数値を文字に変換
         ST      GR2,CNT         ; 出力用バッファに追加
         OUT     BUF,LEN         ; 出力
         SUBA    GR1,=1          ; デクリメント
         JNZ     LOOP            ; 繰り返し処理を続けるかどうか判定
;
         RPOP
         RET
;
LEN      DC      9
BUF      DC      'Count = '
CNT      DC      ' '
         END
