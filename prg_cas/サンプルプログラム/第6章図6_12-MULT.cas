;ファイル名をMULT.CASに変更するか，
;MULT.CASにコピーして使用してください．
;
MULT      START
          PUSH    0, GR2
          PUSH    0, GR3
          LAD     GR3, 0, GR1
          LAD     GR1, 0           ;掛算結果用のレジスタをクリア
LOOP      CPA     GR2, =0
          JZE     FIN
          ADDA    GR1, GR3         ;掛けられる数を加える
          LAD     GR2, -1, GR2      ;掛ける数を1減らす
          JUMP    LOOP
FIN       POP     GR3
          POP     GR2
          RET
          END
