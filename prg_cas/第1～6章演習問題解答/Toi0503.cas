TOI0503  START
         RPUSH
;
         LAD     GR1,0           ; カウンタ初期化
LOOP     CPA     GR1,=8
         JPL     FIN
         LD      GR2,='0'        ; 繰返し処理の始まり
         ADDA    GR2,GR1
         ST      GR2,CNT
         OUT     BUF,LEN
         ADDA    GR1,=1
         JUMP    LOOP
;
FIN      RPOP
         RET
;
LEN      DC      9
BUF      DC      'Count = '
CNT      DC      ' '
         END
